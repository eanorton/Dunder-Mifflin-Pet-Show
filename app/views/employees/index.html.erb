<h1>All Employees</h1>
<ul>
  <% @employees.each do |employee| %>
    <li><%= link_to employee.first_name, employee %></li>
  <% end %>
</ul>

<%= link_to "New Employee", new_employee_path %>
<%= link_to "All Dogs", dogs_path %>


<% if @employees.blank? %>
  <h4>There are no employees containing the term <%= params[:search] %>.</h4>
<% end %>
